{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es6"],
    "allowJs": true,
    "outDir": "build",
    "rootDir": "src",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "typeRoots": ["/src/types/custom.d.ts", "/node_modules/@types"]
  },
  "exclude": ["node_modules", "public", "uploads","build","extra"]
  // "files": ["src/types/custom.d.ts"],
}
